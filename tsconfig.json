{
    "compilerOptions": {
        "target": "ES6",
        "module": "ES6",
        "lib": ["ES7", "DOM"],
        "rootDir": "src",
        "outDir": "public/generated",
        "sourceMap": true,
        "noEmitOnError": true,
        "strict": true,
        "removeComments": true,
    },
    "include": ["src/**/*"],
}